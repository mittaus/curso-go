    "version": "0.2.0",
    "configurations": [

        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 9361,
            "showLog": true
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 8080,
            "host": "127.0.0.1"
        },
        {
            "name": "Launch executable",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "/home/mittaus/projects/Experis/GoLang/01_introduccion_al_curso/debug/app/app"
        },
        {
            "name": "Launch local",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/home/mittaus/projects/Experis/GoLang/01_introduccion_al_curso/debug/app/myapp",
            "port": 2345,
            "host": "localhost",
            "cwd": "/home/mittaus/projects/Experis/GoLang/01_introduccion_al_curso/debug/app",
            "showLog": true,
            "trace": "verbose",
        },
        {
            "name": "Launch remote",
